<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hodo.iiot.group2.data.hdcloud.cash.account.base.mapper.cambodia.HdCashRemarkCambodiaMapper">

  <resultMap id="hdCashRemarkMap" type="com.hodo.iiot.group2.data.hdcloud.cash.account.base.entity.HdCashRemark">
                  <id property="id" column="id"/>
                        <result property="remark" column="remark"/>
                        <result property="createBy" column="create_by"/>
                        <result property="createById" column="create_by_id"/>
                        <result property="tenantId" column="tenant_id"/>
            </resultMap>
    <select id="selectDistinctString" resultType="java.lang.String">
        select distinct ${str} from hd_cash_remark where ${param} = #{val} and tenant_id = #{tenantId}
    </select>
</mapper>
