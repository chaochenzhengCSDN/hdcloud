<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hodo.iiot.group2.data.hdcloud.bank.account.base.mapper.HdCompanyMemberMapper">

    <resultMap id="hdCompanyMemberMap" type="com.hodo.iiot.group2.data.hdcloud.bank.account.base.entity.HdCompanyMember">
        <id property="id" column="id"/>
        <result property="companyId" column="company_id"/>
        <result property="companyName" column="company_name"/>
        <result property="code" column="code"/>
        <result property="name" column="name"/>
        <result property="tenantId" column="tenant_id"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
        <result property="companyCode" column="company_code"/>
        <result property="createById" column="create_by_id"/>
    </resultMap>

    <select id="selectDistinctString" resultType="java.lang.String">
        select distinct ${str} from hd_company_member where ${param} = #{val} and tenant_id = #{tenantId}
    </select>
</mapper>
